{
    "manifest_version": 2,
  
    "name": "Space Rocket Game",
    "description": "This extension is a cool rocket game can play when you bored on Chrome",
    "version": "1.0",
  
    "background": {
         "scripts": ["keysToMove.js"]
        }, 
    "browser_action": {
     "default_icon": "rocket.png",
     "default_popup": "index.html"
    },
    "content_scripts": [{
        "css": ["style.css"],
        "matches": [
            "<all_urls>"
          ]
      }], 
    "permissions": [
     "activeTab"
     ]
  }